{
  "name": "alexw23/laravel-sns-broadcaster",
  "description": "Sns Messages as queues using extended laravel sqs driver.",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/alexw23/laravel-sns-broadcaster",
  "authors": [
    {
      "name": "Alex Whiteside",
      "email": "alexwhiteside@ignitedlabs.com.au"
    }
  ],
  "keywords": [
    "sns",
    "laravel",
    "broadcasting",
    "aws"
  ],
  "require": {
    "php": ">=7.4 <8.4",
    "ext-json": "*",
    "illuminate/broadcasting": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/contracts": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "aws/aws-sdk-php": "^3.305"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MaxGaurav\\LaravelSnsBroadcaster\\LaravelSnsBroadcastProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "MaxGaurav\\LaravelSnsBroadcaster\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
